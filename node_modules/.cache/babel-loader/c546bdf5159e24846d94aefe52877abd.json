\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '员工门禁授权' }), key: \"employeeAccessControlAuthorization\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '申请钥匙' }), key: \"requestKey\",\r\n            },\r\n        ]\r\n    },\r\n    // 疫情\r\n    {\r\n        label: \"疫情\",\r\n        key: \"epidemic\",\r\n        icon: renderIcon(SyringeIcon),\r\n        children: [\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '疫情设置' }), key: \"epidemicSettings\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '返省上报' }), key: \"reportBackToTheProvince\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '疫情上报' }), key: \"epidemicReport\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '外来人员登记' }), key: \"registrationOfForeigners\",\r\n            },\r\n        ]\r\n    }\r\n];\r\n// 侧边栏宽度\r\nconst leftWidth = 180;\r\n// 侧边栏是否打开\r\nconst leftOpen = ref(true);\r\n// 侧边栏模式\r\nconst leftStatus = ref(false);\r\n// 侧边栏颜色\r\nconst leftBlack = ref(true);\r\n\r\n\r\n// 窗口变化时触发\r\nwindow.onresize = () => {\r\n    return (() => {\r\n        initWindow();\r\n    })()\r\n}\r\n\r\n// 初始化窗口函数\r\nconst initWindow = () => {\r\n    // 获取窗口宽度\r\n    let width = document.body.clientWidth\r\n    if (width < 1024) {\r\n        leftOpen.value = false;\r\n        leftStatus.value = false;\r\n    } else {\r\n        leftOpen.value = true;\r\n        leftStatus.value = true;\r\n    }\r\n}\r\n// 初始化窗口\r\ninitWindow();\r\n\r\n</script>\r\n<script>\r\nexport default {\r\n    name: 'MainLayout'\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.app {\r\n    display: flex;\r\n    flex-direction: row;\r\n}\r\n\r\n.app>.left {\r\n    flex: none;\r\n    z-index: 1;\r\n    height: 100vh;\r\n    box-shadow: 5px 0 10px var(--divider-color);\r\n    border-right: 1px solid var(--divider-color);\r\n}\r\n\r\n.app>.right {\r\n    flex: 1;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}