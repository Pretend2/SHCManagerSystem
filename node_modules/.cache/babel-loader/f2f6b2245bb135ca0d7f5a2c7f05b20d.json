ct(params, more, ['dataRange']);\n      touRegLoading.value = true;\n      axios({\n        method: 'get',\n        url: global.api + '/getAllVisitorRecords',\n        params\n      }).then(res => {\n        pageNo.value = res.data.pageNo; // 设置当前页码\n\n        pages.value = res.data.pages; // 设置总页数\n\n        touRegData.value = res.data.list; // 设置访客登记数据\n      }).catch(err => {\n        message.error('加载访客信息出现异常！' + err);\n      }).finally(() => {\n        touRegLoading.value = false;\n      });\n    };\n\n    loadTouRegData();\n    /**\r\n     * 页码发生变化时触发\r\n     * @param {页码} page \r\n     */\n\n    const touRegPageChange = page => {\n      const more = {\n        page\n      };\n      mergeObject(more, findData.value);\n      loadTouRegData(more);\n    }; // #endregion\n\n\n    const __returned__ = {\n      global,\n      message,\n      dialog,\n      findData,\n      findTouRegData,\n      resetTouReg,\n      touRegData,\n      pageNo,\n      pages,\n      touRegLoading,\n      loadTouRegData,\n      touRegPageChange,\n      ref,\n      inject,\n      computed,\n      NCard,\n      NSpace,\n      NInput,\n      NButton,\n      NTable,\n      NIcon,\n      NPagination,\n      NButtonGroup,\n      NEllipsis,\n      NSelect,\n      NDatePicker,\n      useMessage,\n      useDialog,\n      NSpin,\n      AddIcon,\n      EditIcon,\n      DeleteIcon,\n      SearchIcon,\n      ArrowClockwiseIcon,\n      CaretDownIcon,\n      axios,\n      formatDate,\n      mergeObject\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n});","map":{"version":3,"mappings":"AA+NA,oBAAe;EACbA,IAAI,EAAE;AADO,CAAf;AA9GA,SAASC,GAAT,EAAcC,MAAd,EAAsBC,QAAtB,QAAsC,KAAtC,C,CACA;;AACA,SACEC,KADF,EACSC,MADT,EACiBC,MADjB,EACyBC,OADzB,EACkCC,MADlC,EAEEC,KAFF,EAESC,WAFT,EAEsBC,YAFtB,EAGEC,SAHF,EAGaC,OAHb,EAGsBC,WAHtB,EAGmCC,UAHnC,EAIEC,SAJF,EAIaC,KAJb,QAKO,UALP,C,CAMA;;AACA,SACEC,YAAY,IAAIC,OADlB