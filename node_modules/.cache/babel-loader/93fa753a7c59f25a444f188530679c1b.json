{"ast":null,"code":"const __default__ = {\n  name: 'LoginLayout'\n};\nimport { ref, reactive } from 'vue'; // 引入 naive-ui 组件\n\nimport { NForm, NFormItem, NButton, NInput, NCheckbox, NInputGroup, useMessage } from 'naive-ui'; // 引入图形验证码组件\n\nimport imageCode from '../../components/ImageCode.vue'; // 引入 md5 插件\n\nimport md5 from 'md5';\nimport axios from 'axios'; // 验证码数据\n\nexport default /*#__PURE__*/Object.assign(__default__, {\n  setup(__props, {\n    expose\n  }) {\n    expose(); // 引入 vue\n\n    const data = reactive({\n      change_img_code: false,\n      // 刷新验证码\n      img_code: '' // 加密后的验证码值\n\n    }); // 刷新验证码操作\n\n    const changeImageCode = () => {\n      data.change_img_code = !data.change_img_code;\n    }; // 接收组件返回加密后的验证码值\n\n\n    const backImageCode = code => {\n      data.img_code = code;\n    };\n\n    const formRef = ref(null);\n    const message = useMessage();\n    const formValue = ref({\n      account: \"\",\n      password: \"\",\n      code: \"\",\n      autoLogin: false\n    });\n    const rules = {\n      account: {\n        required: true,\n        message: \"请输入账号\",\n        trigger: [\"input\", \"blur\"]\n      },\n      password: {\n        required: true,\n        message: \"请输入密码\",\n        trigger: [\"input\", \"blur\"]\n      },\n      code: {\n        required: true,\n        message: \"请输入验证码\",\n        trigger: [\"input\", \"blur\"]\n      }\n    }; // 登录按钮点事件\n\n    const loginClick = e => {\n      e.preventDefault();\n      formRef.value?.validate(errors => {\n        if (!errors) {\n          if (data.img_code === md5(formValue.value.code)) {\n            // message.success(\"验证码校验成功！\");\n            changeImageCode(); // 刷新验证码\n\n            login(); // 登录\n          } else {\n            message.warning(\"验证码输入错误！\");\n          }\n        }\n      });\n    }; // 登录\n\n\n    function login() {\n      axios({\n        url: '/api/login',\n        params: {\n          account: formValue.value.account,\n          password: formValue.value.password\n        }\n      }).then(res => {\n        console.log(res);\n      }).catch(err => {\n        console.log(err);\n      });\n    }\n\n    const __returned__ = {\n      data,\n      changeImageCode,\n      backImageCode,\n      formRef,\n      message,\n      formValue,\n      rules,\n      loginClick,\n      login,\n      ref,\n      reactive,\n      NForm,\n      NFormItem,\n      NButton,\n      NInput,\n      NCheckbox,\n      NInputGroup,\n      useMessage,\n      imageCode,\n      md5,\n      axios\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n});","map":{"version":3,"mappings":"AAgIA,oBAAe;EACXA,IAAI,EAAE;AADK,CAAf;AA1FA,SAASC,GAAT,EAAcC,QAAd,QAA8B,KAA9B,C,CACA;;AACA,SACIC,KADJ,EACWC,SADX,EACsBC,OADtB,EAC+BC,MAD/B,EACuCC,SADvC,EAEIC,WAFJ,EAEiBC,UAFjB,QAGO,UAHP,C,CAIA;;AACA,OAAOC,SAAP,MAAsB,gCAAtB,C,CACA;;AACA,OAAOC,GAAP,MAAgB,KAAhB;AACA,OAAOC,KAAP,MAAkB,OAAlB,C,CAEA;;;;;;cAbA;;IAcA,MAAMC,IAAI,GAAGX,QAAQ,CAAC;MAClBY,eAAe,EAAE,KADC;MACM;MACxBC,QAAQ,EAAE,EAFQ,CAEL;;IAFK,CAAD,CAArB,EAKA;;IACA,MAAMC,eAAe,GAAG,MAAM;MAC1BH,IAAI,CAACC,eAAL,GAAuB,CAACD,IAAI,CAACC,eAA7B;IACH,CAFD,EAIA;;;IACA,MAAMG,aAAa,GAAIC,IAAD,IAAU;MAC5BL,IAAI,CAACE,QAAL,GAAgBG,IAAhB;IACH,CAFD;;IAKA,MAAMC,OAAO,GAAGlB,GAAG,CAAC,IAAD,CAAnB;IACA,MAAMmB,OAAO,GAAGX,UAAU,EAA1B;IAEA,MAAMY,SAAS,GAAGpB,GAAG,CAAC;MAClBqB,OAAO,EAAE,EADS;MAElBC,QAAQ,EAAE,EAFQ;MAGlBL,IAAI,EAAE,EAHY;MAIlBM,SAAS,EAAE;IAJO,CAAD,CAArB;IAMA,MAAMC,KAAK,GAAG;MACVH,OAAO,EAAE;QACLI,QAAQ,EAAE,IADL;QAELN,OAAO,EAAE,OAFJ;QAGLO,OAAO,EAAE,CAAC,OAAD,EAAU,MAAV;MAHJ,CADC;MAMVJ,QAAQ,EAAE;QACNG,QAAQ,EAAE,IADJ;QAENN,OAAO,EAAE,OAFH;QAGNO,OAAO,EAAE,CAAC,OAAD,EAAU,MAAV;MAHH,CANA;MAWVT,IAAI,EAAE;QACFQ,QAAQ,EAAE,IADR;QAEFN,OAAO,EAAE,QAFP;QAGFO,OAAO,EAAE,CAAC,OAAD,EAAU,MAAV;MAHP;IAXI,CAAd,EAkBA;;IACA,MAAMC,UAAU,GAAIC,CAAD,IAAO;MACtBA,CAAC,CAACC,cAAF;MACAX,OAAO,CAACY,KAAR,EAAeC,QAAf,CAAyBC,MAAD,IAAY;QAChC,IAAI,CAACA,MAA