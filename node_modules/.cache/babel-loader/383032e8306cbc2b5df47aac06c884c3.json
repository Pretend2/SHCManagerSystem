sed-width=\"64\"\r\n                            :collapsed-icon-size=\"22\" />\r\n                    </n-scrollbar>\r\n                </n-layout-sider>\r\n            </n-layout>\r\n        </n-layout>\r\n        <div class=\"right\">\r\n            <ActionBar @menuClick=\"leftOpen = !leftOpen\" :menuStatus=\"leftOpen\" :menuEnable=\"true\">\r\n            </ActionBar>\r\n            <router-view></router-view>\r\n        </div>\r\n    </n-el>\r\n</template>\r\n\r\n<script setup>\r\n// 引入 vue\r\nimport { ref, h, computed } from 'vue'\r\n// 引入 naive ui 组件\r\nimport {\r\n    NButton, NSpace, NGrid, NGridItem, NEl,\r\n    NIcon, NMenu, NLayoutSider, NLayout, NDrawer,\r\n    NDrawerContent, NScrollbar\r\n} from 'naive-ui'\r\n// 引入 应用栏组件\r\nimport ActionBar from '@/components/ActionBar.vue'\r\n// 引入路由\r\nimport { RouterLink } from 'vue-router'\r\n// 引入图像\r\nimport {\r\n    Home20Regular as HomeIcon,\r\n    Building20Regular as Home20Regular,\r\n    Money20Regular as MoneyIcon,\r\n    BezierCurveSquare20Regular as BezierCurveSquareIcon,\r\n    VehicleCarProfileLtr20Regular as VehicleCarProfileLtrIcon,\r\n    CalendarEdit20Regular as CalendarEditIcon,\r\n    ShoppingBag20Regular as ShoppingBagIcon,\r\n    DeveloperBoard20Regular as DeveloperBoardIcon,\r\n    Syringe20Regular as SyringeIcon\r\n} from \"@vicons/fluent\"\r\n\r\n// 图标渲染\r\nfunction renderIcon(icon) {\r\n    return () => h(NIcon, null, { default: () => h(icon) });\r\n}\r\n// 侧边栏数据\r\nconst menuOptions = [\r\n    // 首页\r\n    {\r\n        label: \"首页\",\r\n        key: \"home\",\r\n        icon: renderIcon(HomeIcon)\r\n    },\r\n    // 房产\r\n    {\r\n        label: '房产',\r\n        key: \"realEstate\",\r\n        icon: renderIcon(Home20Regular),\r\n        children: [\r\n            {\r\n                label: () => h(RouterLink, { to: '/realestate' }, { default: () => '房屋管理' }), key: \"housingManagement\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '商铺管理' }), key: \"shopManagement\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '业主信息' }), key: \"ownerInformation\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '业主成员' }), key: \"ownerMember\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '绑定业主' }), key: \"bindTheOwner\",\r\n            },\r\n        ]\r\n    },\r\n    // 费用\r\n    {\r\n        label: \"费用\",\r\n        key: \"cost\",\r\n        icon: renderIcon(MoneyIcon),\r\n        children: [\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '费用项设置' }), key: \"expenseItemSettings\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '费用套餐' }), key: \"feePackage\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '房屋收费' }), key: \"housingHarges\",\r\n            },\r\n        ]\r\n    },\r\n    // 办公\r\n    {\r\n        label: \"办公\",\r\n        key: \"office\",\r\n        icon: renderIcon(BezierCurveSquareIcon),\r\n        children: [\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '发布公告' }), key: \"annouce\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '电话投诉' }), key: \"phoneComplaint\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '游客登记' }), key: \"touristRegistration\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '活动规则' }), key: \"ruleOfActivity\",\r\n            },\r\n        ]\r\n    },\r\n    // 停车\r\n    {\r\n        label: \"停车\",\r\n        key: \"parking\",\r\n        icon: renderIcon(VehicleCarProfileLtrIcon),\r\n        children: [\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '停车场管理' }), key: \"parkingLotManagement\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '车位管理' }), key: \"parkingSpaceManagement\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '岗亭管理' }), key: \"boothManagement\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '业主车辆' }), key: \"ownerVehicle\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '进场记录' }), key: \"approachRecord\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '停车记录' }), key: \"parkingRecord\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '黑白名单' }), key: \"blackAndWhiteList\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '剩余车位' }), key: \"remainingParkingSpaces\",\r\n            },\r\n        ]\r\n    },\r\n    // 报修\r\n    {\r\n        label: \"报修\",\r\n        key: \"repair\",\r\n        icon: renderIcon(CalendarEditIcon),\r\n        children: [\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '保修设置' }), key: \"warrantySettings\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '电话报修' }), key: \"telephoneRepair\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '工单池' }), key: \"ticketPool\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '报修待办' }), key: \"repairPending\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '报修已办' }), key: \"repairHasBeenDone\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '报修回访' }), key: \"repairVisit\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '强制回访' }), key: \"mandatoryReturnVisit\",\r\n            },\r\n        ]\r\n    },\r\n    // 采购\r\n    {\r\n        label: \"采购\",\r\n        key: \"purchase\",\r\n        icon: renderIcon(ShoppingBagIcon),\r\n        children: [\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '仓库信息' }), key: \"warehouseInformation\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '物品信息' }), key: \"itemInformation\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '物品类型' }), key: \"itemType\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '物品供应商' }), key: \"itemupplier\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '物品规格' }), key: \"ItemSpecifications\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '采购申请' }), key: \"purchaseRequisition\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '出入库明细' }), key: \"inboundAndOutboundDetails\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '物品使用记录' }), key: \"itemUsageRecord\",\r\n            },\r\n        ]\r\n    },\r\n    // 设备\r\n    {\r\n        label: \"设备\",\r\n        key: \"equipment\",\r\n        icon: renderIcon(DeveloperBoardIcon),\r\n        children: [\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '设备位置' }), key: \"deviceLocation\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '设备类型' }), key: \"equipmentType\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '设备信息' }), key: \"deviceInformation\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '门禁设备' }), key: \"accessControlEquipment\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '车辆道闸' }), key: \"vehicleBarrier\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '监控设备' }), key: \"monitoringEquipment\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '考勤机' }), key: \"attendanceMachine\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '开门记录' }), key: \"openDoorRecord\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '员工门禁授权' }), key: \"employeeAccessControlAuthorization\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '申请钥匙' }), key: \"requestKey\",\r\n            },\r\n        ]\r\n    },\r\n    // 疫情\r\n    {\r\n        label: \"疫情\",\r\n        key: \"epidemic\",\r\n        icon: renderIcon(SyringeIcon),\r\n        children: [\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '疫情设置' }), key: \"epidemicSettings\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '返省上报' }), key: \"reportBackToTheProvince\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '疫情上报' }), key: \"epidemicReport\",\r\n            },\r\n            {\r\n                label: () => h(RouterLink, { to: '/' }, { default: () => '外来人员登记' }), key: \"registrationOfForeigners\",\r\n            },\r\n        ]\r\n    }\r\n];\r\n// 侧边栏宽度\r\nconst leftWidth = 180;\r\n// 侧边栏是否打开\r\nconst leftOpen = ref(true);\r\n// 侧边栏模式\r\nconst leftStatus = ref(false);\r\n// 侧边栏颜色\r\nconst leftBlack = ref(false);\r\n\r\n// 窗口变化时触发\r\nwindow.onresize = () => {\r\n    return (() => {\r\n        initWindow();\r\n    })()\r\n}\r\n\r\n// 初始化窗口函数\r\nconst initWindow = () => {\r\n    // 获取窗口宽度\r\n    let width = document.body.clientWidth\r\n    if (width < 1024) {\r\n        leftOpen.value = false\r\n        leftStatus.value = false;\r\n    } else {\r\n        leftOpen.value = true\r\n        leftStatus.value = true;\r\n    }\r\n}\r\n// 初始化窗口\r\ninitWindow();\r\n\r\n</script>\r\n<script>\r\nexport default {\r\n    name: 'MainLayout'\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.app {\r\n    display: flex;\r\n    flex-direction: row;\r\n}\r\n\r\n.app>.left {\r\n    z-index: 1;\r\n    height: 100vh;\r\n    box-shadow: 5px 0 10px var(--divider-color);\r\n    border-right: 1px solid var(--divider-color);\r\n}\r\n\r\n.app>.right {\r\n    flex: 1;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}