= ref(1); // 总页数\n\n    const pages = ref(1);\n    /**\r\n     * 查询公告信息\r\n     */\n\n    function findAnnouncementData(more) {\n      console.log(more);\n      announcementLoading.value = true;\n      const params = {\n        page: 1,\n        size: global.pageSize\n      };\n      mergeObject(params, more);\n      axios({\n        url: global.api + '/getAllAppAnnouncement',\n        method: 'get',\n        params\n      }).then(res => {\n        // 设置数据\n        announcementData.value = res.data.list; // 设置当前页码\n\n        pageNo.value = res.data.pageNum; // 设置总页数\n\n        pages.value = res.data.pages;\n      }).catch(err => {\n        message.error(err);\n      }).finally(() => {\n        announcementLoading.value = false;\n      });\n    } // 加载信息\n\n\n    findAnnouncementData(); // #endregion\n\n    const __returned__ = {\n      global,\n      message,\n      typeOptions,\n      statusOptions,\n      announcementFindData,\n      findAnnouncement,\n      announcementData,\n      announcementLoading,\n      pageNo,\n      pages,\n      findAnnouncementData,\n      ref,\n      inject,\n      NCard,\n      NSpace,\n      NInput,\n      NButton,\n      NTable,\n      NIcon,\n      NPagination,\n      NButtonGroup,\n      useDialog,\n      NEllipsis,\n      NSelect,\n      NDatePicker,\n      useMessage,\n      NSpin,\n      AddIcon,\n      EditIcon,\n      DeleteIcon,\n      SearchIcon,\n      ArrowClockwiseIcon,\n      CaretDownIcon,\n      axios,\n      mergeObject\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n});","map":{"version":3,"mappings":"AAwMA,oBAAe;EACbA,IAAI,EAAE;AADO,CAAf;AA1GA,SAASC,GAAT,EAAcC,MAAd,QAA4B,KAA5B,C,CACA;;AACA,SACEC,KADF,EACSC,MADT,EACiBC,MADjB,EACyBC,OADzB,EACkCC,MADlC,EAEEC,KAFF,EAESC,WAFT,EAEsBC,YAFtB,EAEoCC,SAFpC,EAGEC,SAHF,EAGaC,OAHb,EAGsBC,WAHtB,EAGmCC,UAHnC,EAIEC,KAJF,QAKO,UALP,C,CAMA;;AACA,SACEC,YAAY,IAAIC,OADlB,EAEEC,a