{"ast":null,"code":"const __default__ = {\n  name: 'PhoneComplaintLayout'\n};\nimport { ref, computed, inject } from 'vue'; // 引入 naive-ui 组件\n\nimport { NCard, NSpace, NInput, NButton, NTable, NIcon, NPagination, NButtonGroup, NEllipsis, NSelect, NDatePicker, useMessage, NSpin, NForm, NFormItem, NGridItem, NGrid } from 'naive-ui'; // 引入图标 \n\nimport { Add16Regular as AddIcon, Edit16Regular as EditIcon, Delete16Regular as DeleteIcon, Search16Regular as SearchIcon, ArrowClockwise16Regular as ArrowClockwiseIcon, CaretDown12Filled as CaretDownIcon } from '@vicons/fluent'; // 引入 HousingOption\n\nimport HousingOption from '../../components/HousingOption.vue';\nimport axios from 'axios';\nimport { mergeObject } from '../../util/BaseUtil'; // 读取全局变量\n\nexport default /*#__PURE__*/Object.assign(__default__, {\n  setup(__props, {\n    expose\n  }) {\n    expose(); // 引入 vue\n\n    const global = inject('global'); // 获取消息对象\n\n    const message = useMessage(); // #region 查询条件\n\n    const findComplaints = ref({\n      complaintsNo: null,\n      status: null,\n      type: null\n    }); // 状态数组\n\n    const statusOption = ['投诉', '建议', '咨询'].map((k, v) => ({\n      label: k,\n      value: v\n    })); // 类型数组\n\n    const typeOption = ['处理中', '处理完成'].map((k, v) => ({\n      label: k,\n      value: v\n    }));\n    /**\r\n     * 查询数据\r\n     */\n\n    const findData = () => {\n      pageChange(1);\n    };\n    /**\r\n     * 清空查询条件数据\r\n     */\n\n\n    const resetData = () => {\n      findComplaints.value = {\n        complaintsNo: null,\n        status: null,\n        type: null\n      };\n    }; // #endregion\n    // #region 查询投诉信息\n\n\n    const phoneComplaintData = ref([]); // 电话头数数据\n\n    const pageNo = ref(1); // 当前页码\n\n    const pages = ref(1); // 总页数\n\n    const phoneComplaintLoading = ref(false); // 电话头数加载\n\n    /**\r\n     * 查询电话投诉信息\r\n     */\n\n    function findPhoneComplaint(more) {\n      phoneComplaintLoading.value = true;\n      const params = {\n        page: 1,\n        size: global.pageSize\n      };\n      mergeObject(params, more);\n      axios({\n        method: 'get',\n        url: global.api + '/getAllPhoneComplaints',\n        params\n      }).then(res => {\n        // 设置数据\n        phoneComplaintData.value = res.data.list; // 设置当前页码\n\n        pageNo.value = res.data.pageNum; // 设置总页数\n\n        pages.value = res.data.pages;\n      }).catch(err => {\n        message.error('电话投诉信息加载出现异常！' + err);\n      }).finally(() => {\n        phoneComplaintLoading.value = false;\n      });\n    }\n\n    findPhoneComplaint(); // 加载列表\n\n    /**\r\n     * 页码发生变化时事件\r\n     */\n\n    const pageChange = page => {\n      // 切换页面相关代码\n      findPhoneComplaint({\n        page,\n        complaintsNo: findComplaints.value.complaintsNo,\n        status: findComplaints.value.status,\n        type: findComplaints.value.type\n      });\n    }; // #endregion\n    // #region 添加投诉\n\n\n    const phoneComplaintOptionShow = ref(false); // 显示添加投诉模态框\n\n    const housingInfo = ref({\n      data: null\n    }); // 房屋信息\n    // #endregion\n\n    const __returned__ = {\n      global,\n      message,\n      findComplaints,\n      statusOption,\n      typeOption,\n      findData,\n      resetData,\n      phoneComplaintData,\n      pageNo,\n      pages,\n      phoneComplaintLoading,\n      findPhoneComplaint,\n      pageChange,\n      phoneComplaintOptionShow,\n      housingInfo,\n      ref,\n      computed,\n      inject,\n      NCard,\n      NSpace,\n      NInput,\n      NButton,\n      NTable,\n      NIcon,\n      NPagination,\n      NButtonGroup,\n      NEllipsis,\n      NSelect,\n      NDatePicker,\n      useMessage,\n      NSpin,\n      NForm,\n      NFormItem,\n      NGridItem,\n      NGrid,\n      AddIcon,\n      EditIcon,\n      DeleteIcon,\n      SearchIcon,\n      ArrowClockwiseIcon,\n      CaretDownIcon,\n      HousingOption,\n      axios,\n      mergeObject\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n});","map":{"version":3,"mappings":"AA0QA,oBAAe;EACXA,IAAI,EAAE;AADK,CAAf;AA3HA,SAASC,GAAT,EAAcC,QAAd,EAAwBC,MAAxB,QAAsC,KAAtC,C,CACA;;AACA,SACIC,KADJ,EACWC,MADX,EACmBC,MADnB,EAC2BC,OAD3B,EACoCC,MADpC,EAEIC,KAFJ,EAEWC,WAFX,EAEwBC,YAFxB,EAGIC,SAHJ,EAGeC,OAHf,EAGwBC,WAHxB,EAGqCC,UAHrC,EAIIC,KAJJ,EAIWC,KAJX,EAIkBC,SAJlB,EAI6BC,SAJ7B,EAIwCC,KAJxC,QAKO,UALP,C,CAMA;;AACA,SACIC,YAAY,IAAIC,OADpB,EAEIC,aAAa,IAAIC,QAFrB,EAGIC,eAAe,IAAIC,UAHvB,EAIIC,eAAe,IAAIC,UAJvB,EAKIC,uBAAuB,IAAIC,kBAL/B,EAMIC,iBAAiB,IAAIC,aANzB,QAOO,gBAPP,C,CASA;;AACA,OAAOC,aAAP,MAA0B,oCAA1B;AAEA,OAAOC,KAAP,MAAkB,OAAlB;AACA,SAASC,WAAT,QAA4B,qBAA5B,C,CAEA;;;;;;cAzBA;;IA0BA,MAAMC,MAAM,GAAGjC,MAAM,CAAC,QAAD,CAArB,EACA;;IACA,MAAMkC,OAAO,GAAGtB,UAAU,EAA1B,EAEA;;IACA,MAAMuB,cAAc,GAAGrC,GAAG,CAAC;MACvBsC,YAAY,EAAE,IADS;MAEvBC,MAAM,EAAE,IAFe;MAGvBC,IAAI,EAAE;IAHiB,CAAD,CAA1B,EAMA;;IACA,MAAMC,YAAY,GAAG,CAAC,IAAD,EAAO,IAAP,EAAa,IAAb,EAAmBC,GAAnB,CAAuB,CAACC,CAAD,EAAIC,CAAJ,MAAW;MACnDC,KAAK,EAAEF,CAD4C;MAEnDG,KAAK,EAAEF;IAF4C,CAAX,CAAvB,CAArB,EAKA;;IACA,MAAMG,UAAU,GAAG,CAAC,KAAD,EAAQ,MAAR,EAAgBL,GAAhB,CAAoB,CAACC,CAAD,EAAIC,CAAJ,MAAW;MAC9CC,KAAK,EAAEF,CADuC;MAE9CG,KAAK,EAAEF;IAFuC,CAAX,CAApB,CAAnB;IAKA;AACA;AACA;;IACA,MAAMI,QAAQ,GAAG,MAAM;MACnBC,UAAU,CAAC,CAAD,CAAV;IACH,CAFD;IAIA;AACA;AACA;;;IACA,MAAMC,SAAS,GAAG,MAAM;MACpBb,cAAc,CAACS,KAAf,GAAuB;QACnBR,YAAY,EAAE,IADK;QAEnBC,MAAM,EAAE,IAFW;QAGnBC,IAAI,EAAE;MAHa,CAAvB;IAKH,CAND,EAOA;IAEA;;;IACA,MAAMW,kBAAkB,GAAGnD,GAAG,CAAC,EAAD,CAA9B,EAAmC;;IACnC,MAAMoD,MAAM,GAAGpD,GAAG,CAAC,CAAD,CAAlB,EAAsB;;IACtB,MAAMqD,KAAK,GAAGrD,GAAG,CAAC,CAAD,CAAjB,EAAqB;;IACrB,MAAMsD,qBAAqB,GAAGtD,GAAG,CAAC,KAAD,CAAjC,EAAyC;;IACzC;AACA;AACA;;IACA,SAASuD,kBAAT,CAA4BC,IAA5B,EAAkC;MAC9BF,qBAAqB,CAACR,KAAtB,GAA8B,IAA9B;MACA,MAAMW,MAAM,GAAG;QACXC,IAAI,EAAE,CADK;QAEXC,IAAI,EAAExB,MAAM,CAACyB;MAFF,CAAf;MAIA1B,WAAW,CAACuB,MAAD,EAASD,IAAT,CAAX;MACAvB,KAAK,CAAC;QACF4B,MAAM,EAAE,KADN;QAEFC,GAAG,EAAE3B,MAAM,CAAC4B,GAAP,GAAa,wBAFhB;QAGFN;MAHE,CAAD,CAAL,CAIGO,IAJH,CAIQC,GAAG,IAAI;QACX;QACAd,kBAAkB,CAACL,KAAnB,GAA2BmB,GAAG,CAACC,IAAJ,CAASC,IAApC,CAFW,CAGX;;QACAf,MAAM,CAACN,KAAP,GAAemB,GAAG,CAACC,IAAJ,CAASE,OAAxB,CAJW,CAKX;;QACAf,KAAK,CAACP,KAAN,GAAcmB,GAAG,CAACC,IAAJ,CAASb,KAAvB;MACH,CAXD,EAWGgB,KAXH,CAWSC,GAAG,IAAI;QACZlC,OAAO,CAACmC,KAAR,CAAc,kBAAkBD,GAAhC;MACH,CAbD,EAaGE,OAbH,CAaW,MAAM;QACblB,qBAAqB,CAACR,KAAtB,GAA8B,KAA9B;MACH,CAfD;IAgBH;;IACDS,kBAAkB,IAAG;;IAErB;AACA;AACA;;IACA,MAAMN,UAAU,GAAIS,IAAD,IAAU;MACzB;MACAH,kBAAkB,CAAC;QACfG,IADe;QAEfpB,YAAY,EAAED,cAAc,CAACS,KAAf,CAAqBR,YAFpB;QAGfC,MAAM,EAAEF,cAAc,CAACS,KAAf,CAAqBP,MAHd;QAIfC,IAAI,EAAEH,cAAc,CAACS,KAAf,CAAqBN;MAJZ,CAAD,CAAlB;IAMH,CARD,EAUA;IAEA;;;IACA,MAAMiC,wBAAwB,GAAGzE,GAAG,CAAC,KAAD,CAApC,EAA4C;;IAC5C,MAAM0E,WAAW,GAAG1E,GAAG,CAAC;MAAEkE,IAAI,EAAE;IAAR,CAAD,CAAvB,EAAwC;IACxC","names":["name","ref","computed","inject","NCard","NSpace","NInput","NButton","NTable","NIcon","NPagination","NButtonGroup","NEllipsis","NSelect","NDatePicker","useMessage","NSpin","NForm","NFormItem","NGridItem","NGrid","Add16Regular","AddIcon","Edit16Regular","EditIcon","Delete16Regular","DeleteIcon","Search16Regular","SearchIcon","ArrowClockwise16Regular","ArrowClockwiseIcon","CaretDown12Filled","CaretDownIcon","HousingOption","axios","mergeObject","global","message","findComplaints","complaintsNo","status","type","statusOption","map","k","v","label","value","typeOption","findData","pageChange","resetData","phoneComplaintData","pageNo","pages","phoneComplaintLoading","findPhoneComplaint","more","params","page","size","pageSize","method","url","api","then","res","data","list","pageNum","catch","err","error","finally","phoneComplaintOptionShow","housingInfo"],"sources":["E:/VCode/SHC社区管理系统/shc_community_manager_system/src/layout/office/PhoneComplaintLayout.vue"],"sourcesContent":["<template>\r\n    <div style=\"padding: 0 10px\">\r\n        <n-card title=\"查询条件\" style=\"margin-top: 10px\">\r\n            <n-space item-style=\"display: flex; align-item: center;\">\r\n                <n-input v-model:value=\"findComplaints.complaintsNo\" style=\"width: 160px\" type=\"text\"\r\n                    placeholder=\"请输入投诉ID\" />\r\n                <n-select clearable v-model:value=\"findComplaints.type\" placeholder=\"请选择投诉类型\" style=\"width:160px\"\r\n                    :options=\"typeOption\" />\r\n                <n-select clearable v-model:value=\"findComplaints.status\" placeholder=\"请选择投诉状态\" style=\"width:160px\"\r\n                    :options=\"statusOption\" />\r\n                <n-button type=\"primary\" @click=\"findData\">\r\n                    <template #icon>\r\n                        <n-icon>\r\n                            <search-icon />\r\n                        </n-icon>\r\n                    </template>\r\n                    查询\r\n                </n-button>\r\n                <n-button @click=\"resetData\">\r\n                    <template #icon>\r\n                        <n-icon>\r\n                            <arrow-clockwise-icon />\r\n                        </n-icon>\r\n                    </template>\r\n                    重置\r\n                </n-button>\r\n            </n-space>\r\n        </n-card>\r\n\r\n        <n-spin :show=\"phoneComplaintLoading\">\r\n            <n-card title=\"投诉建议\" style=\"margin-top: 10px;padding: 0; margin-bottom: 10px;\">\r\n                <template #header-extra>\r\n                    <n-space>\r\n                        <n-button @click=\"phoneComplaintOptionShow = true\" type=\"primary\">\r\n                            <template #icon>\r\n                                <n-icon>\r\n                                    <add-icon />\r\n                                </n-icon>\r\n                            </template>\r\n                            投诉\r\n                        </n-button>\r\n                    </n-space>\r\n                </template>\r\n                <n-table style=\"margin-top: 10px\" striped>\r\n                    <thead>\r\n                        <tr>\r\n                            <th>投诉ID</th>\r\n                            <th>投诉类型</th>\r\n                            <th>房屋</th>\r\n                            <th>投诉人</th>\r\n                            <th>投诉人电话</th>\r\n                            <th>投诉状态</th>\r\n                            <th>处理人</th>\r\n                            <th>处理人电话</th>\r\n                            <th>创建时间</th>\r\n                            <th>操作</th>\r\n                        </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                        <tr v-for=\"(item, index) in phoneComplaintData\" :key=\"index\">\r\n                            <td>\r\n                                <n-ellipsis style=\"max-width: 100px\">\r\n                                    {{ item.complaintsNo }}\r\n                                </n-ellipsis>\r\n                            </td>\r\n                            <td>\r\n                                {{ item.type }}\r\n                            </td>\r\n                            <td>\r\n                                <n-ellipsis style=\"max-width: 100px\">\r\n                                    {{ item.buildingNo + \"栋\" + item.housesNo + \"室\" }}\r\n                                </n-ellipsis>\r\n                            </td>\r\n                            <td>\r\n                                {{ item.complainantsName }}\r\n                            </td>\r\n                            <td>\r\n                                <n-ellipsis style=\"max-width: 100px\">\r\n                                    {{ item.phone }}\r\n                                </n-ellipsis>\r\n                            </td>\r\n                            <td>{{ item.status === 0 ? '未处理' : '已处理' }}</td>\r\n                            <td>{{ item.handler != null ? item.handler.name : '' }}</td>\r\n                            <td>\r\n                                <n-ellipsis style=\"max-width: 100px\">\r\n                                    {{ item.handler != null ? item.handler.phone : '' }}\r\n                                </n-ellipsis>\r\n                            </td>\r\n                            <td>\r\n                                {{ item.phoneComplaintHandled != null ? item.phoneComplaintHandled.handleTime : '' }}\r\n                            </td>\r\n                            <td>\r\n                                <n-button-group size=\"large\">\r\n                                    <n-button size=\"small\">详情</n-button>\r\n                                    <n-button size=\"small\">修改</n-button>\r\n                                    <n-button type=\"error\" size=\"small\" strong secondary>删除</n-button>\r\n                                </n-button-group>\r\n                            </td>\r\n                        </tr>\r\n                    </tbody>\r\n                </n-table>\r\n\r\n                <div v-if=\"pages > 1\"\r\n                    style=\"display: flex;justify-content: center;align-items: center;margin-top: 24px;\">\r\n                    <n-pagination @update:page=\"pageChange\" :page-count=\"pages\" />\r\n                </div>\r\n\r\n            </n-card>\r\n        </n-spin>\r\n\r\n        <!-- 投诉模态框 -->\r\n        <HousingOption req-url=\"/getHouses/\" :show=\"phoneComplaintOptionShow\" :data=\"housingInfo\">\r\n            <template #content>\r\n                <n-form ref=\"annoFormRef\" label-placement=\"left\" :label-width=\"120\" :model=\"annoValue\"\r\n                    :rules=\"annoRules\" size=\"medium\">\r\n                    <n-grid cols=\"1\">\r\n                        <n-grid-item>\r\n                            <n-form-item label=\"投诉类型\" path=\"\">\r\n                                <n-input placeholder=\"请输入公告标题\" />\r\n                            </n-form-item>\r\n                            <n-form-item label=\"投诉人姓名\" path=\"\">\r\n                                <n-input placeholder=\"请输入投诉人姓名\" />\r\n                            </n-form-item>\r\n                            <n-form-item label=\"投诉人电话\" path=\"\">\r\n                                <n-input placeholder=\"请输入投诉人电话\" />\r\n                            </n-form-item>\r\n                        </n-grid-item>\r\n                    </n-grid>\r\n                </n-form>\r\n            </template>\r\n            <template #footer>\r\n                <n-space justify=\"end\">\r\n                    <n-button @click=\"phoneComplaintOptionShow = false\">取消</n-button>\r\n                    <n-button type=\"primary\" attr-type=\"button\">选择</n-button>\r\n                </n-space>\r\n            </template>\r\n        </HousingOption>\r\n\r\n    </div>\r\n</template>\r\n\r\n<script setup>\r\n// 引入 vue\r\nimport { ref, computed, inject } from 'vue'\r\n// 引入 naive-ui 组件\r\nimport {\r\n    NCard, NSpace, NInput, NButton, NTable,\r\n    NIcon, NPagination, NButtonGroup,\r\n    NEllipsis, NSelect, NDatePicker, useMessage,\r\n    NSpin, NForm, NFormItem, NGridItem, NGrid\r\n} from 'naive-ui'\r\n// 引入图标 \r\nimport {\r\n    Add16Regular as AddIcon,\r\n    Edit16Regular as EditIcon,\r\n    Delete16Regular as DeleteIcon,\r\n    Search16Regular as SearchIcon,\r\n    ArrowClockwise16Regular as ArrowClockwiseIcon,\r\n    CaretDown12Filled as CaretDownIcon,\r\n} from '@vicons/fluent'\r\n\r\n// 引入 HousingOption\r\nimport HousingOption from '../../components/HousingOption.vue'\r\n\r\nimport axios from 'axios'\r\nimport { mergeObject } from '../../util/BaseUtil'\r\n\r\n// 读取全局变量\r\nconst global = inject('global')\r\n// 获取消息对象\r\nconst message = useMessage()\r\n\r\n// #region 查询条件\r\nconst findComplaints = ref({\r\n    complaintsNo: null,\r\n    status: null,\r\n    type: null,\r\n})\r\n\r\n// 状态数组\r\nconst statusOption = ['投诉', '建议', '咨询'].map((k, v) => ({\r\n    label: k,\r\n    value: v,\r\n}))\r\n\r\n// 类型数组\r\nconst typeOption = ['处理中', '处理完成'].map((k, v) => ({\r\n    label: k,\r\n    value: v,\r\n}))\r\n\r\n/**\r\n * 查询数据\r\n */\r\nconst findData = () => {\r\n    pageChange(1)\r\n}\r\n\r\n/**\r\n * 清空查询条件数据\r\n */\r\nconst resetData = () => {\r\n    findComplaints.value = {\r\n        complaintsNo: null,\r\n        status: null,\r\n        type: null,\r\n    }\r\n}\r\n// #endregion\r\n\r\n// #region 查询投诉信息\r\nconst phoneComplaintData = ref([]) // 电话头数数据\r\nconst pageNo = ref(1) // 当前页码\r\nconst pages = ref(1) // 总页数\r\nconst phoneComplaintLoading = ref(false) // 电话头数加载\r\n/**\r\n * 查询电话投诉信息\r\n */\r\nfunction findPhoneComplaint(more) {\r\n    phoneComplaintLoading.value = true\r\n    const params = {\r\n        page: 1,\r\n        size: global.pageSize\r\n    }\r\n    mergeObject(params, more)\r\n    axios({\r\n        method: 'get',\r\n        url: global.api + '/getAllPhoneComplaints',\r\n        params\r\n    }).then(res => {\r\n        // 设置数据\r\n        phoneComplaintData.value = res.data.list\r\n        // 设置当前页码\r\n        pageNo.value = res.data.pageNum\r\n        // 设置总页数\r\n        pages.value = res.data.pages\r\n    }).catch(err => {\r\n        message.error('电话投诉信息加载出现异常！' + err)\r\n    }).finally(() => {\r\n        phoneComplaintLoading.value = false\r\n    })\r\n}\r\nfindPhoneComplaint() // 加载列表\r\n\r\n/**\r\n * 页码发生变化时事件\r\n */\r\nconst pageChange = (page) => {\r\n    // 切换页面相关代码\r\n    findPhoneComplaint({\r\n        page,\r\n        complaintsNo: findComplaints.value.complaintsNo,\r\n        status: findComplaints.value.status,\r\n        type: findComplaints.value.type\r\n    })\r\n}\r\n\r\n// #endregion\r\n\r\n// #region 添加投诉\r\nconst phoneComplaintOptionShow = ref(false) // 显示添加投诉模态框\r\nconst housingInfo = ref({ data: null }) // 房屋信息\r\n// #endregion\r\n\r\n</script>\r\n<script>\r\nexport default {\r\n    name: 'PhoneComplaintLayout'\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}