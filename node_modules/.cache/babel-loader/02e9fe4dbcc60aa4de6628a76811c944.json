{"ast":null,"code":"const __default__ = {\n  name: 'OwnerInformationLayout'\n};\nimport { ref, inject, reactive } from 'vue'; // 引入 naive-ui 组件\n\nimport { NCard, NSpace, NInput, NButton, NTable, NIcon, NPagination, NButtonGroup, NDropdown, NEllipsis, NModal, NGrid, NGridItem, NForm, NFormItem, useMessage, NSelect, NSpin, NList, NListItem } from 'naive-ui'; // 引入图标 \n\nimport { Add16Regular as AddIcon, Edit16Regular as EditIcon, Delete16Regular as DeleteIcon, Search16Regular as SearchIcon, ArrowClockwise16Regular as ArrowClockwiseIcon, CaretDown12Filled as CaretDownIcon } from '@vicons/fluent'; // 引入业主 Hook\n\nimport ownerHook from '../../hook/real_estate/ownerInformation/ownerHook'; // 引入加载业主 Hook\n\nimport loadOwnerHook from '../../hook/real_estate/ownerInformation/loadOwnerHook'; // 引入 HousingOption\n\nimport HousingOption from '../../components/HousingOption.vue';\nimport axios from 'axios';\nexport default /*#__PURE__*/Object.assign(__default__, {\n  setup(__props, {\n    expose\n  }) {\n    expose(); // 引入 vue\n\n    const housingOptionShow = ref(false); // 接收全局变量\n\n    const global = inject(\"global\"); // 加载业主\n\n    const {\n      ownerPageNo,\n      // 业主数据页码\n      ownerPages,\n      // 业主数据总页数\n      ownerData,\n      // 业主数据\n      ownerLoading,\n      // 业主加载\n      loadOwnerInfo,\n      // 加载业主信息\n      ownerPageUpdate // 页码改变时事件\n\n    } = loadOwnerHook(); // 业主信息\n\n    const {\n      ownerStatus,\n      // 模态 添加 or 修改 框状态\n      ownerFormRef,\n      // 业主 Form 引用\n      showOwnerModal,\n      // 显示模态框\n      ownerRules,\n      // 表单校验\n      ownerformValue,\n      // 表单参数\n      ownerGenderOptions,\n      // 性别选项数据\n      ownerModelLoading,\n      // 业主加载\n      openOwnerModal,\n      // 打开模态框\n      closeOwnerModal,\n      // 关闭模态框\n      ownerSubmit,\n      // 表单提交\n      deleteOwner // 删除业主信息\n\n    } = ownerHook(refreshOwnerData); // 业主变更选项\n\n    const options = [{\n      label: \"修改业主\",\n      key: 0\n    }, {\n      label: \"删除业主\",\n      key: 1\n    }, {\n      label: \"入住房屋\",\n      key: 2\n    }, {\n      label: \"房屋解绑\",\n      key: 3\n    }]; // 开始加载业主信息\n\n    loadOwnerInfo(1); // 创建消息提示对象\n\n    const message = useMessage(); // 业主查询条件\n\n    const ownerFindValue = ref({\n      name: '',\n      phone: '',\n      idCard: ''\n    });\n    /**\r\n     * 查询按钮点击事件\r\n     */\n\n    const findClick = () => {\n      loadOwnerInfo(1, ownerFindValue.value);\n    };\n    /**\r\n     * 刷新业主数据\r\n     */\n\n\n    function refreshOwnerData() {\n      loadOwnerInfo(ownerPageNo.value, ownerFindValue.value);\n    }\n    /**\r\n     * 重置查询条件\r\n     */\n\n\n    const resetClick = () => {\n      ownerFindValue.value = {\n        name: '',\n        phone: '',\n        idCard: ''\n      };\n    }; // 业主信息\n\n\n    let ownerInfo = null; // 房屋信息\n\n    const housingInfo = ref({\n      data: null\n    });\n    /**\r\n     * 列表项被选中时触发\r\n     */\n\n    const ownerMoreSelect = (key, item) => {\n      switch (key) {\n        case 0:\n          // 修改业主信息\n          openOwnerModal(true, item);\n          break;\n\n        case 1:\n          // 删除业主信息\n          deleteOwner(item.id, item.name);\n          break;\n\n        case 2:\n          // 入住房屋\n          housingOptionShow.value = true;\n          ownerInfo = item;\n          break;\n\n        case 3:\n          // 房屋解绑\n          openHousingUnbundlingOrInfo(false, item);\n          break;\n      }\n    }; // 房屋入住加载\n\n\n    const housingCheckInLoading = ref(false);\n    /**\r\n     * 房屋入住\r\n     */\n\n    const housingCheckIn = () => {\n      if (housingInfo.value.data != null) {\n        housingCheckInLoading.value = true;\n        axios({\n          method: 'put',\n          url: global.api + '/bindHouses/' + housingInfo.value.data.id + '/' + ownerInfo.ownerNo\n        }).then(res => {\n          if (res.data.success) {\n            housingOptionShow.value = false;\n            message.success(\"入住成功！\");\n            ownerInfo = null;\n          } else {\n            message.warning(\"入住失败！\");\n          }\n        }).catch(err => {\n          message.error('入住出现异常！' + err);\n        }).finally(() => {\n          housingCheckInLoading.value = false;\n        });\n      } else {\n        message.warning(\"请选择房屋！\");\n      }\n    }; // 显示房屋解绑模态框\n\n\n    const showHousingUnbundlingOrInfo = ref(false); // 模态框状态 false 解绑房屋 true 业主信息\n\n    const housingUnbundlingOrInfoStatus = ref(false); // 业主信息\n\n    const ownerInfoData = ref({\n      \"id\": null,\n      \"ownerNo\": null,\n      \"name\": null,\n      \"gender\": null,\n      \"age\": null,\n      \"phone\": null,\n      \"idCard\": null,\n      \"weCat\": null,\n      \"remark\": null,\n      \"isMember\": null,\n      \"memberNo\": null\n    });\n\n    const openHousingUnbundlingOrInfo = (status, owner) => {\n      ownerInfoData.value = owner;\n      showHousingUnbundlingOrInfo.value = true;\n      housingUnbundlingOrInfoStatus.value = status;\n    };\n\n    const __returned__ = {\n      housingOptionShow,\n      global,\n      ownerPageNo,\n      ownerPages,\n      ownerData,\n      ownerLoading,\n      loadOwnerInfo,\n      ownerPageUpdate,\n      ownerStatus,\n      ownerFormRef,\n      showOwnerModal,\n      ownerRules,\n      ownerformValue,\n      ownerGenderOptions,\n      ownerModelLoading,\n      openOwnerModal,\n      closeOwnerModal,\n      ownerSubmit,\n      deleteOwner,\n      options,\n      message,\n      ownerFindValue,\n      findClick,\n      refreshOwnerData,\n      resetClick,\n      ownerInfo,\n      housingInfo,\n      ownerMoreSelect,\n      housingCheckInLoading,\n      housingCheckIn,\n      showHousingUnbundlingOrInfo,\n      housingUnbundlingOrInfoStatus,\n      ownerInfoData,\n      openHousingUnbundlingOrInfo,\n      ref,\n      inject,\n      reactive,\n      NCard,\n      NSpace,\n      NInput,\n      NButton,\n      NTable,\n      NIcon,\n      NPagination,\n      NButtonGroup,\n      NDropdown,\n      NEllipsis,\n      NModal,\n      NGrid,\n      NGridItem,\n      NForm,\n      NFormItem,\n      useMessage,\n      NSelect,\n      NSpin,\n      NList,\n      NListItem,\n      AddIcon,\n      EditIcon,\n      DeleteIcon,\n      SearchIcon,\n      ArrowClockwiseIcon,\n      CaretDownIcon,\n      ownerHook,\n      loadOwnerHook,\n      HousingOption,\n      axios\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n});","map":{"version":3,"mappings":"AAiaA,oBAAe;EACXA,IAAI,EAAE;AADK,CAAf;AArMA,SAASC,GAAT,EAAcC,MAAd,EAAsBC,QAAtB,QAAsC,KAAtC,C,CACA;;AACA,SACIC,KADJ,EACWC,MADX,EACmBC,MADnB,EAC2BC,OAD3B,EACoCC,MADpC,EAEIC,KAFJ,EAEWC,WAFX,EAEwBC,YAFxB,EAEsCC,SAFtC,EAGIC,SAHJ,EAGeC,MAHf,EAGuBC,KAHvB,EAG8BC,SAH9B,EAGyCC,KAHzC,EAIIC,SAJJ,EAIeC,UAJf,EAI2BC,OAJ3B,EAIoCC,KAJpC,EAI2CC,KAJ3C,EAIkDC,SAJlD,QAKO,UALP,C,CAMA;;AACA,SACIC,YAAY,IAAIC,OADpB,EAEIC,aAAa,IAAIC,QAFrB,EAGIC,eAAe,IAAIC,UAHvB,EAIIC,eAAe,IAAIC,UAJvB,EAKIC,uBAAuB,IAAIC,kBAL/B,EAMIC,iBAAiB,IAAIC,aANzB,QAOO,gBAPP,C,CASA;;AACA,OAAOC,SAAP,MAAsB,mDAAtB,C,CACA;;AACA,OAAOC,aAAP,MAA0B,uDAA1B,C,CACA;;AACA,OAAOC,aAAP,MAA0B,oCAA1B;AAEA,OAAOC,KAAP,MAAkB,OAAlB;;;;;cA1BA;;IA4BA,MAAMC,iBAAiB,GAAGvC,GAAG,CAAC,KAAD,CAA7B,EAEA;;IACA,MAAMwC,MAAM,GAAGvC,MAAM,CAAC,QAAD,CAArB,EAEA;;IACA,MAAM;MACFwC,WADE;MACW;MACbC,UAFE;MAEU;MACZC,SAHE;MAGS;MACXC,YAJE;MAIY;MACdC,aALE;MAKa;MACfC,eANE,CAMe;;IANf,IAOFV,aAAa,EAPjB,EAQA;;IACA,MAAM;MACFW,WADE;MACW;MACbC,YAFE;MAEY;MACdC,cAHE;MAGc;MAChBC,UAJE;MAIU;MACZC,cALE;MAKc;MAChBC,kBANE;MAMkB;MACpBC,iBAPE;MAOiB;MACnBC,cARE;MAQc;MAChBC,eATE;MASe;MACjBC,WAVE;MAUW;MACbC,WAXE,CAWW;;IAXX,IAYFtB,SAAS,CAACuB,gBAAD,CAZb,EAcA;;IACA,MAAMC,OAAO,GAAG,CACZ;MACIC,KAAK,EAAE,MADX;MAEIC,GAAG,EAAE;IAFT,CADY,EAKZ;MACID,KAAK,EAAE,MADX;MAEIC,GAAG,EAAE;IAFT,CALY,EASZ;MACID,KAAK,EAAE,MADX;MAEIC,GAAG,EAAE;IAFT,CATY,EAaZ;MACID,KAAK,EAAE,MADX;MAEIC,GAAG,EAAE;IAFT,CAbY,CAAhB,EAmBA;;IACAhB,aAAa,CAAC,CAAD,CAAb,EAEA;;IACA,MAAMiB,OAAO,GAAG5C,UAAU,EAA1B,EAEA;;IA