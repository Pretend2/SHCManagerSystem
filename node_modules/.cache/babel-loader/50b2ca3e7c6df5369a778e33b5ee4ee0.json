on>\r\n      <n-button>\r\n        <template #icon>\r\n          <n-icon>\r\n            <delete-icon />\r\n          </n-icon>\r\n        </template>\r\n        删除单元\r\n      </n-button>\r\n      <!-- 添加房屋 -->\r\n      <n-button type=\"primary\">\r\n        <template #icon>\r\n          <n-icon>\r\n            <add-icon />\r\n          </n-icon>\r\n        </template>\r\n        添加房屋\r\n      </n-button>\r\n    </div>\r\n\r\n    <div class=\"main\">\r\n      <div class=\"left\">\r\n        <n-card class=\"left-card\">\r\n          <n-tree :node-props=\"nodeProps\" block-line :data=\"data\" default-expand-all virtual-scroll />\r\n        </n-card>\r\n      </div>\r\n      <div class=\"right\">\r\n        <n-card title=\"查询条件\">\r\n          <n-space item-style=\"display: flex; align-item: center;\">\r\n            <n-input style=\"width:256px\" type=\"text\" placeholder=\"请填写房屋编号格式楼栋-单元-房屋\" />\r\n            <n-select placeholder=\"请选择状态\" style=\"width:160px\" :options=\"options\" />\r\n            <n-select placeholder=\"请选择房屋类型\" style=\"width:160px\" :options=\"options\" />\r\n            <n-button type=\"primary\">\r\n              <template #icon>\r\n                <n-icon>\r\n                  <search-icon />\r\n                </n-icon>\r\n              </template>\r\n              查询\r\n            </n-button>\r\n            <n-button>\r\n              <template #icon>\r\n                <n-icon>\r\n                  <arrow-clockwise-icon />\r\n                </n-icon>\r\n              </template>\r\n              重置\r\n            </n-button>\r\n          </n-space>\r\n        </n-card>\r\n\r\n        <n-table style=\"margin-top: 10px\" striped>\r\n          <thead>\r\n            <tr>\r\n              <th>房屋</th>\r\n              <th>楼层</th>\r\n              <th>业主</th>\r\n              <th>电话</th>\r\n              <th>类型</th>\r\n              <th>建筑面积</th>\r\n              <th>室内面积</th>\r\n              <th>租金</th>\r\n              <th>入住时间</th>\r\n  