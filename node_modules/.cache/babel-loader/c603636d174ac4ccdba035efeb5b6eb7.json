nst addSubmit = (e) => {\r\n    e.preventDefault();\r\n    formRef.value?.validate((errors) => {\r\n        if (!errors) {\r\n            message.success(\"Valid\");\r\n        } else {\r\n            console.log(errors);\r\n            message.error(\"Invalid\");\r\n        }\r\n    });\r\n}\r\n\r\n</script>\r\n\r\n<script>\r\nexport default {\r\n    name: 'OwnerInformationLayout'\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}