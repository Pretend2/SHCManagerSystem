\n            </ActionBar>\r\n        </div>\r\n    </n-el>\r\n</template>\r\n\r\n<script setup>\r\n// 引入 vue\r\nimport { ref, h, onMounted, onUnmounted } from 'vue'\r\n// 引入 naive ui 组件\r\nimport {\r\n    NButton, NSpace, NGrid, NGridItem, NEl,\r\n    NIcon, NMenu, NLayoutSider, NLayout, NDrawer,\r\n    NDrawerContent\r\n} from 'naive-ui'\r\n// 引入 应用栏组件\r\nimport ActionBar from '@/components/ActionBar.vue'\r\n\r\n// 引入图像\r\nimport {\r\n    HomeOutlined as HomeIcon,\r\n} from \"@vicons/antd\";\r\n\r\n// 图标渲染\r\nfunction renderIcon(icon) {\r\n    return () => h(NIcon, null, { default: () => h(icon) });\r\n}\r\n\r\n// 侧边栏数据\r\nconst menuOptions = [\r\n    {\r\n        label: \"首页\",\r\n        key: \"home\",\r\n        icon: renderIcon(HomeIcon)\r\n    },\r\n    {\r\n        label: \"1973年的弹珠玩具\",\r\n        key: \"pinball-1973\",\r\n        icon: renderIcon(HomeIcon),\r\n        disabled: true,\r\n        children: [\r\n            {\r\n                label: \"鼠\",\r\n                key: \"rat\"\r\n            }\r\n        ]\r\n    },\r\n    {\r\n        label: \"寻羊冒险记\",\r\n        key: \"a-wild-sheep-chase\",\r\n        disabled: true,\r\n        icon: renderIcon(HomeIcon)\r\n    },\r\n    {\r\n        label: \"舞，舞，舞\",\r\n        key: \"dance-dance-dance\",\r\n        icon: renderIcon(HomeIcon),\r\n        children: [\r\n            {\r\n                type: \"group\",\r\n                label: \"人物\",\r\n                key: \"people\",\r\n                children: [\r\n                    {\r\n                        label: \"叙事者\",\r\n                        key: \"narrator\",\r\n                        icon: renderIcon(HomeIcon)\r\n                    },\r\n                    {\r\n                        label: \"羊男\",\r\n                        key: \"sheep-man\",\r\n                        icon: renderIcon(HomeIcon)\r\n                    }\r\n                ]\r\n            },\r\n            {\r\n                label: \"饮品\",\r\n                key: \"beverage\",\r\n                icon: renderIcon(HomeIcon),\r\n                children: [\r\n                    {\r\n                        label: \"威士忌\",\r\n                        key: \"whisky\"\r\n                    }\r\n                ]\r\n            },\r\n            {\r\n                label: \"食物\",\r\n                key: \"food\",\r\n                children: [\r\n                    {\r\n                        label: \"三明治\",\r\n                        key: \"sandwich\"\r\n                    }\r\n                ]\r\n            },\r\n            {\r\n                label: \"过去增多，未来减少\",\r\n                key: \"the-past-increases-the-future-recedes\"\r\n            }\r\n        ]\r\n    }\r\n];\r\n// 侧边栏是否打开\r\nconst leftOpen = ref(true);\r\n// 侧边栏模式\r\nconst leftStatus = ref(false);\r\n\r\n// 窗口变化时触发\r\nwindow.onresize = () => {\r\n    return (() => {\r\n        initWindow();\r\n    })()\r\n}\r\n\r\n// 初始化窗口函数\r\nconst initWindow = () => {\r\n    // 获取窗口宽度\r\n    let width = document.body.clientWidth\r\n    if (width < 1024) {\r\n        leftOpen.value = false\r\n        leftStatus.value = false;\r\n    } else {\r\n        leftOpen.value = true\r\n        leftStatus.value = true;\r\n    }\r\n}\r\n// 初始化窗口\r\ninitWindow();\r\n\r\n</script>\r\n<script>\r\nexport default {\r\n    name: 'MainLayout'\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.app {\r\n    display: flex;\r\n    flex-direction: row;\r\n}\r\n\r\n.app>.left {\r\n    height: 100vh;\r\n    background-color: var(--card-color);\r\n    border-right: 1px solid var(--divider-color);\r\n}\r\n\r\n.app>.right {\r\n    flex: 1;\r\n}\r\n\r\n.n-menu-item-content {\r\n    line-height: 0;\r\n}\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}