geNo = ref(1); // 当前页码\n\n    const pages = ref(1); // 总页数\n\n    const phoneComplaintLoading = ref(false); // 电话头数加载\n\n    /**\r\n     * 查询电话投诉信息\r\n     */\n\n    function findPhoneComplaint(more) {\n      phoneComplaintLoading.value = true;\n      const params = {\n        page: 1,\n        size: global.pageSize\n      };\n      mergeObject(params, more);\n      axios({\n        method: 'get',\n        url: global.api + '/getAllPhoneComplaints',\n        params\n      }).then(res => {\n        // 设置数据\n        phoneComplaintData.value = res.data.list; // 设置当前页码\n\n        pageNo.value = res.data.pageNum; // 设置总页数\n\n        pages.value = res.data.pages;\n      }).catch(err => {\n        message.error('电话投诉信息加载出现异常！' + err);\n      }).finally(() => {\n        phoneComplaintLoading.value = false;\n      });\n    }\n\n    findPhoneComplaint(); // 加载列表\n\n    /**\r\n     * 页码发生变化时事件\r\n     */\n\n    const pageChange = page => {// 切换页面相关代码\n    }; // #endregion\n    // #region 添加投诉\n\n\n    const phoneComplaintOptionShow = ref(false); // 显示添加投诉模态框\n\n    const housingInfo = ref({\n      data: null\n    }); // 房屋信息\n    // #endregion\n\n    const __returned__ = {\n      global,\n      message,\n      options,\n      phoneComplaintData,\n      pageNo,\n      pages,\n      phoneComplaintLoading,\n      findPhoneComplaint,\n      pageChange,\n      phoneComplaintOptionShow,\n      housingInfo,\n      ref,\n      computed,\n      inject,\n      NCard,\n      NSpace,\n      NInput,\n      NButton,\n      NTable,\n      NIcon,\n      NPagination,\n      NButtonGroup,\n      NEllipsis,\n      NSelect,\n      NDatePicker,\n      useMessage,\n      NSpin,\n      AddIcon,\n      EditIcon,\n      DeleteIcon,\n      SearchIcon,\n      ArrowClockwiseIcon,\n      CaretDownIcon,\n      HousingOption,\n      axios,\n      mergeObject\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n