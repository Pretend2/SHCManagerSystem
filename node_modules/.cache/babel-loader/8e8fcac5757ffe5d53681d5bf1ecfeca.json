 }).catch(err => {\r\n        message.error('电话投诉信息加载出现异常！' + err)\r\n    }).finally(() => {\r\n        phoneComplaintLoading.value = false\r\n    })\r\n}\r\nfindPhoneComplaint() // 加载列表\r\n\r\n/**\r\n * 页码发生变化时事件\r\n */\r\nconst pageChange = (page) => {\r\n    // 切换页面相关代码\r\n}\r\n\r\n// #endregion\r\n\r\n// #region 添加投诉\r\nconst phoneComplaintOptionShow = ref(false) // 显示添加投诉模态框\r\nconst housingInfo = ref({data:null}) // 房屋信息\r\n// #endregion\r\n\r\n</script>\r\n<script>\r\nexport default {\r\n    name: 'PhoneComplaintLayout'\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}