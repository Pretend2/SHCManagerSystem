          <n-button type=\"primary\">\r\n                    <template #icon>\r\n                        <n-icon>\r\n                            <search-icon />\r\n                        </n-icon>\r\n                    </template>\r\n                    查询\r\n                </n-button>\r\n                <n-button>\r\n                    <template #icon>\r\n                        <n-icon>\r\n                            <arrow-clockwise-icon />\r\n                        </n-icon>\r\n                    </template>\r\n                    重置\r\n                </n-button>\r\n            </n-space>\r\n        </n-card>\r\n\r\n        <n-spin :show=\"phoneComplaintLoading\">\r\n            <n-card title=\"投诉建议\" style=\"margin-top: 10px;padding: 0; margin-bottom: 10px;\">\r\n                <template #header-extra>\r\n                    <n-space>\r\n                        <n-button @click=\"phoneComplaintOptionShow = true\" type=\"primary\">\r\n                            <template #icon>\r\n                                <n-icon>\r\n                                    <add-icon />\r\n                                </n-icon>\r\n                            </template>\r\n                            投诉\r\n                        </n-button>\r\n                    </n-space>\r\n                </template>\r\n                <n-table style=\"margin-top: 10px\" striped>\r\n                    <thead>\r\n                        <tr>\r\n                            <th>投诉ID</th>\r\n                            <th>投诉类型</th>\r\n                            <th>房屋</th>\r\n                            <th>投诉人</th>\r\n                            <th>投诉人电话</th>\r\n                            <th>投诉状态</th>\r\n                            <th>处理人</th>\r\n                            <th>处理人电话</th>\r\n                            <th>创建时间</th>\r\n                            <th>操作</th>\r\n                        </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                        <tr v-for=\"(item,index) in phoneComplaintData\" :key=\"index\">\r\n                            <td>\r\n                                <n-ellipsis style=\"max-width: 100px\">\r\n                                    {{item.complaintsNo}}\r\n                                </n-ellipsis>\r\n                            </td>\r\n                            <td>\r\n                                {{item.type}}\r\n                            </td>\r\n                            <td>\r\n                                <n-ellipsis style=\"max-width: 100px\">\r\n                                    {{item.buildingNo + \"栋\" + item.housesNo + \"室\"}}\r\n                                </n-ellipsis>\r\n                            </td>\r\n                            <td>\r\n                                {{item.complainantsName}}\r\n                            </td>\r\n                            <td>\r\n                                <n-ellipsis style=\"max-width: 100px\">\r\n                                    {{item.phone}}\r\n                                </n-ellipsis>\r\n                            </td>\r\n                            <td>{{item.status === 0 ? '未处理' : '已处理'}}</td>\r\n                            <td>{{item.handler != null ? item.handler.name : ''}}</td>\r\n                            <td>\r\n                                <n-ellipsis style=\"max-width: 100px\">\r\n                                    {{item.handler != null ? item.handler.phone : ''}}\r\n                                </n-ellipsis>\r\n                            </td>\r\n                            <td>\r\n                                {{item.phoneComplaintHandled != null ? item.phoneComplaintHandled.handleTime : ''}}\r\n                            </td>\r\n                            <td>\r\n                                <n-button-group size=\"large\">\r\n                                    <n-button size=\"small\">详情</n-button>\r\n                                    <n-button size=\"small\">修改</n-button>\r\n                                    <n-button type=\"error\" size=\"small\" strong secondary>删除</n-button>\r\n                                </n-button-group>\r\n                            </td>\r\n                        </tr>\r\n                    </tbody>\r\n                </n-table>\r\n\r\n                <div v-if=\"pages > 1\" style=\"display: flex;justify-content: center;align-items: center;margin-top: 24px;\">\r\n                    <n-pagination @update:page=\"pageChange\" :page-count=\"pages\" />\r\n                </div>\r\n\r\n            </n-card>\r\n        </n-spin>\r\n\r\n        <!-- 投诉模态框 -->\r\n        <HousingOption :show=\"phoneComplaintOptionShow\" :data=\"housingInfo\">\r\n            <template #footer>\r\n                <n-space justify=\"end\">\r\n                    <n-button @click=\"phoneComplaintOptionShow = false\">取消</n-button>\r\n                    <n-button type=\"primary\"\r\n                        attr-type=\"button\">选择</n-button>\r\n                </n-space>\r\n            </template>\r\n        </HousingOption>\r\n\r\n    </div>\r\n</template>\r\n\r\n<script setup>\r\n// 引入 vue\r\nimport { ref, computed, inject } from 'vue'\r\n// 引入 naive-ui 组件\r\nimport {\r\n    NCard, NSpace, NInput, NButton, NTable,\r\n    NIcon, NPagination, NButtonGroup,\r\n    NEllipsis, NSelect, NDatePicker, useMessage,\r\n    NSpin\r\n} from 'naive-ui'\r\n// 引入图标 \r\nimport {\r\n    Add16Regular as AddIcon,\r\n    Edit16Regular as EditIcon,\r\n    Delete16Regular as DeleteIcon,\r\n    Search16Regular as SearchIcon,\r\n    ArrowClockwise16Regular as ArrowClockwiseIcon,\r\n    CaretDown12Filled as CaretDownIcon,\r\n} from '@vicons/fluent'\r\n\r\n// 引入 HousingOption\r\nimport HousingOption from '../../components/HousingOption.vue'\r\n\r\nimport axios from 'axios'\r\nimport { mergeObject } from '../../util/BaseUtil'\r\n\r\n// 读取全局变量\r\nconst global = inject('global')\r\n// 获取消息对象\r\nconst message = u