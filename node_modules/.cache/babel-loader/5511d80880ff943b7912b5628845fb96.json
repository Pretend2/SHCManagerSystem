</n-button>\r\n      </n-space>\r\n    </n-card>\r\n\r\n    <n-spin :show=\"announcementLoading\">\r\n      <n-card title=\"公告信息\" style=\"margin-top: 10px;padding: 0; margin-bottom: 10px;\">\r\n        <template #header-extra>\r\n          <n-space>\r\n            <n-button type=\"primary\">\r\n              <template #icon>\r\n                <n-icon>\r\n                  <add-icon />\r\n                </n-icon>\r\n              </template>\r\n              发布公告\r\n            </n-button>\r\n          </n-space>\r\n        </template>\r\n        <n-table style=\"margin-top: 10px\" striped>\r\n          <thead>\r\n            <tr>\r\n              <th>公告ID</th>\r\n              <th>标题</th>\r\n              <th>公告类型</th>\r\n              <th>开始时间</th>\r\n              <th>结束时间</th>\r\n              <th>状态</th>\r\n              <th>通知范围</th>\r\n              <th>操作</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"index in 10\" :key=\"index\">\r\n              <td>\r\n                <n-ellipsis style=\"max-width: 100px\">\r\n                  102022101022721215\r\n                </n-ellipsis>\r\n              </td>\r\n              <td>\r\n                <n-ellipsis style=\"max-width: 100px\">\r\n                  AS\r\n                </n-ellipsis>\r\n              </td>\r\n              <td>业主通知</td>\r\n              <td>\r\n                2022-8-10\r\n              </td>\r\n              <td>\r\n                2022-09-29\r\n              </td>\r\n              <td>通知完成</td>\r\n              <td>演示物业（误删）</td>\r\n              <td>\r\n                <n-button-group size=\"large\">\r\n                  <n-button size=\"small\">详情</n-button>\r\n                  <n-button size=\"small\">修改</n-button>\r\n                  <n-button type=\"error\" size=\"small\" strong secondary>删除</n-button>\r\n                </n-button-group>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </n-table>\r\