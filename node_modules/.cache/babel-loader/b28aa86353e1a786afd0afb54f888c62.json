{"ast":null,"code":"const __default__ = {\n  name: 'HousingManagementLayout'\n};\nimport { ref, reactive, computed } from 'vue'; // 引入 naive-ui 组件\n\nimport { NButton, NIcon, NGrid, NGridItem, NTree, NCard, NInput, NSelect, NSpace, NTable, NButtonGroup, NPagination, NModal, NForm, NFormItem, NInputNumber, NSwitch, useMessage, NSpin, useLoadingBar, NResult } from 'naive-ui'; // 引入 axios\n\nimport axios from 'axios'; // 引入图标 \n\nimport { Add16Regular as AddIcon, Edit16Regular as EditIcon, Delete16Regular as DeleteIcon, Search16Regular as SearchIcon, ArrowClockwise16Regular as ArrowClockwiseIcon } from '@vicons/fluent'; // 引入楼栋模态框钩子函数\n\nimport buildingHook from '../../hook/real_estate/housingmanagement/buildingHook'; // 引入单元模态框钩子函数\n\nimport apartmentHook from '../../hook/real_estate/housingmanagement/apartmentHook';\nexport default /*#__PURE__*/Object.assign(__default__, {\n  setup(__props, {\n    expose\n  }) {\n    expose(); // 引入 vue\n\n    const message = useMessage(); // 房屋列表 Spin\n\n    const mainSpin = ref(false); // 楼栋\n\n    const {\n      buildingStatus,\n      buildingFormRef,\n      showBuildingModal,\n      buildingRules,\n      buildingValue,\n      buildingData,\n      leftSpin,\n      buildingNode,\n      unitNode,\n      buildingSubmit,\n      nodeProps,\n      deleteBuilding\n    } = buildingHook(loadList); // 单元\n\n    const {\n      apartmentStatus,\n      apartmentFormRef,\n      showApartmentModal,\n      apartmentRules,\n      apartmentValue,\n      apartmentSubmit\n    } = apartmentHook();\n    const loadingBar = useLoadingBar(); // 房屋列表\n\n    const houseData = ref([]);\n    const houseDataDisplay = computed(() => {\n      return JSON.parse(JSON.stringify(houseData.value)).lenght === 0 || JSON.parse(JSON.stringify(houseData.value)).list.lenght === 0;\n    }); // 房屋分页数据\n\n    const pageNo = ref(1);\n    const pages = ref(1); // 当前页码发生变化时触发\n\n    function pageNoUpdate(page) {\n      loadList(unitNode.value, page);\n    } // 加载房屋列表\n\n\n    function loadList(option, page) {\n      mainSpin.value = true; // 打开加载动画\n      // loadingBar.start()\n\n      axios({\n        method: 'get',\n        url: '/api/getAllHouses',\n        params: {\n          buildingNo: option.building.id,\n          utilNo: option.id,\n          page,\n          size: 2\n        }\n      }).then(res => {\n        // 设置数据\n        houseData.value = res.data; // 设置当前页数\n\n        pageNo.value = res.data.page; // 设置总页数\n\n        pages.value = res.data.pages;\n        mainSpin.value = false; // 取消加载动画\n        // loadingBar.finish()\n      }).catch(err => {// loadingBar.error()\n      });\n    }\n\n    const options = [{\n      label: \"Everybody's Got Something to Hide Except Me and My Monkey\",\n      value: 'song0',\n      disabled: true\n    }, {\n      label: '全部',\n      value: 'song1'\n    }, {\n      label: 'Norwegian Wood',\n      value: 'song2'\n    }, {\n      label: \"You Won't See\",\n      value: 'song3',\n      disabled: true\n    }, {\n      label: 'Nowhere Man',\n      value: 'song4'\n    }, {\n      label: 'Think For Yourself',\n      value: 'song5'\n    }, {\n      label: 'The Word',\n      value: 'song6'\n    }, {\n      label: 'Michelle',\n      value: 'song7',\n      disabled: true\n    }, {\n      label: 'What goes on',\n      value: 'song8'\n    }, {\n      label: 'Girl',\n      value: 'song9'\n    }, {\n      label: \"I'm looking through you\",\n      value: 'song10'\n    }, {\n      label: 'In My Life',\n      value: 'song11'\n    }, {\n      label: 'Wait',\n      value: 'song12'\n    }];\n    const __returned__ = {\n      message,\n      mainSpin,\n      buildingStatus,\n      buildingFormRef,\n      showBuildingModal,\n      buildingRules,\n      buildingValue,\n      buildingData,\n      leftSpin,\n      buildingNode,\n      unitNode,\n      buildingSubmit,\n      nodeProps,\n      deleteBuilding,\n      apartmentStatus,\n      apartmentFormRef,\n      showApartmentModal,\n      apartmentRules,\n      apartmentValue,\n      apartmentSubmit,\n      loadingBar,\n      houseData,\n      houseDataDisplay,\n      pageNo,\n      pages,\n      pageNoUpdate,\n      loadList,\n      options,\n      ref,\n      reactive,\n      computed,\n      NButton,\n      NIcon,\n      NGrid,\n      NGridItem,\n      NTree,\n      NCard,\n      NInput,\n      NSelect,\n      NSpace,\n      NTable,\n      NButtonGroup,\n      NPagination,\n      NModal,\n      NForm,\n      NFormItem,\n      NInputNumber,\n      NSwitch,\n      useMessage,\n      NSpin,\n      useLoadingBar,\n      NResult,\n      axios,\n      AddIcon,\n      EditIcon,\n      DeleteIcon,\n      SearchIcon,\n      ArrowClockwiseIcon,\n      buildingHook,\n      apartmentHook\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n});","map":{"version":3,"mappings":"AAoZA,oBAAe;EACbA,IAAI,EAAE;AADO,CAAf;AArKA,SAASC,GAAT,EAAcC,QAAd,EAAwBC,QAAxB,QAAwC,KAAxC,C,CACA;;AACA,SACEC,OADF,EACWC,KADX,EACkBC,KADlB,EACyBC,SADzB,EACoCC,KADpC,EAC2CC,KAD3C,EAEEC,MAFF,EAEUC,OAFV,EAEmBC,MAFnB,EAE2BC,MAF3B,EAEmCC,YAFnC,EAGEC,WAHF,EAGeC,MAHf,EAGuBC,KAHvB,EAG8BC,SAH9B,EAGyCC,YAHzC,EAIEC,OAJF,EAIWC,UAJX,EAIuBC,KAJvB,EAI8BC,aAJ9B,EAI6CC,OAJ7C,QAKO,UALP,C,CAMA;;AACA,OAAOC,KAAP,MAAkB,OAAlB,C,CACA;;AACA,SACEC,YAAY,IAAIC,OADlB,EAEEC,aAAa,IAAIC,QAFnB,EAGEC,eAAe,IAAIC,UAHrB,EAIEC,eAAe,IAAIC,UAJrB,EAKEC,uBAAuB,IAAIC,kBAL7B,QAMO,gBANP,C,CAQA;;AACA,OAAOC,YAAP,MAAyB,uDAAzB,C,CAEA;;AACA,OAAOC,aAAP,MAA0B,wDAA1B;;;;;cAxBA;;IA0BA,MAAMC,OAAO,GAAGjB,UAAU,EAA1B,EAEA;;IACA,MAAMkB,QAAQ,GAAGtC,GAAG,CAAC,KAAD,CAApB,EAEA;;IACA,MAAM;MACJuC,cADI;MAEJC,eAFI;MAGJC,iBAHI;MAIJC,aAJI;MAKJC,aALI;MAMJC,YANI;MAOJC,QAPI;MAQJC,YARI;MASJC,QATI;MAUJC,cAVI;MAWJC,SAXI;MAYJC;IAZI,IAaFf,YAAY,CAACgB,QAAD,CAbhB,EAgBA;;IACA,MAAM;MACJC,eADI;MAEJC,gBAFI;MAGJC,kBAHI;MAIJC,cAJI;MAKJC,cALI;MAMJC;IANI,IAOFrB,aAAa,EAPjB;IASA,MAAMsB,UAAU,GAAGpC,aAAa,EAAhC,EAEA;;IACA,MAAMqC,SAAS,GAAG3D,GAAG,CAAC,EA