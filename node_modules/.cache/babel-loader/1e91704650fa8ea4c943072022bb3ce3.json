{"ast":null,"code":"const __default__ = {\n  name: 'PhoneComplaintLayout'\n};\nimport { ref, computed, inject } from 'vue'; // 引入 naive-ui 组件\n\nimport { NCard, NSpace, NInput, NButton, NTable, NIcon, NPagination, NButtonGroup, NEllipsis, NSelect, NDatePicker, useMessage, NSpin, NForm, NFormItem, NGridItem, NGrid } from 'naive-ui'; // 引入图标 \n\nimport { Add16Regular as AddIcon, Edit16Regular as EditIcon, Delete16Regular as DeleteIcon, Search16Regular as SearchIcon, ArrowClockwise16Regular as ArrowClockwiseIcon, CaretDown12Filled as CaretDownIcon } from '@vicons/fluent'; // 引入 HousingOption\n\nimport HousingOption from '../../components/HousingOption.vue';\nimport axios from 'axios';\nimport { mergeObject, createId, formatDate } from '../../util/BaseUtil';\nimport QueryString from 'qs'; // 读取全局变量\n\nexport default /*#__PURE__*/Object.assign(__default__, {\n  setup(__props, {\n    expose\n  }) {\n    expose(); // 引入 vue\n\n    const global = inject('global'); // 获取消息对象\n\n    const message = useMessage(); // #region 查询条件\n\n    const findComplaints = ref({\n      complaintsNo: null,\n      status: null,\n      type: null\n    }); // 状态数组\n\n    const typeOption = ['投诉', '建议', '咨询'].map((k, v) => ({\n      label: k,\n      value: v\n    })); // 类型数组\n\n    const statusOption = ['未处理', '已处理'].map((k, v) => ({\n      label: k,\n      value: v\n    }));\n    /**\r\n     * 查询数据\r\n     */\n\n    const findData = () => {\n      pageChange(1);\n    };\n    /**\r\n     * 清空查询条件数据\r\n     */\n\n\n    const resetData = () => {\n      findComplaints.value = {\n        complaintsNo: null,\n        status: null,\n        type: null\n      };\n    }; // #endregion\n    // #region 查询投诉信息\n\n\n    const phoneComplaintData = ref([]); // 电话头数数据\n\n    const pageNo = ref(1); // 当前页码\n\n    const pages = ref(1); // 总页数\n\n    const phoneComplaintLoading = ref(false); // 电话头数加载\n\n    /**\r\n     * 查询电话投诉信息\r\n     */\n\n    function findPhoneComplaint(more) {\n      phoneComplaintLoading.value = true;\n      const params = {\n        page: 1,\n        size: global.pageSize\n      };\n      mergeObject(params, more);\n      axios({\n        method: 'get',\n        url: global.api + '/getAllPhoneComplaints',\n        params\n      }).then(res => {\n        // 设置数据\n        phoneComplaintData.value = res.data.list; // 设置当前页码\n\n        pageNo.value = res.data.pageNum; // 设置总页数\n\n        pages.value = res.data.pages;\n      }).catch(err => {\n        message.error('电话投诉信息加载出现异常！' + err);\n      }).finally(() => {\n        phoneComplaintLoading.value = false;\n      });\n    }\n\n    findPhoneComplaint(); // 加载列表\n\n    /**\r\n     * 页码发生变化时事件\r\n     */\n\n    const pageChange = page => {\n      // 切换页面相关代码\n      findPhoneComplaint({\n        page,\n        complaintsNo: findComplaints.value.complaintsNo,\n        status: findComplaints.value.status,\n        type: findComplaints.value.type\n      });\n    }; // #endregion\n    // #region 投诉模态框\n\n\n    const phoneComplaintOptionShow = ref(false); // 显示投诉模态框\n\n    const phoneComplaintFormRef = ref(null); // form 表单引用\n\n    const phoneComplaintModalType = ref(false); // 模态框类型\n\n    const housingInfo = ref({\n      data: null\n    }); // 房屋信息\n    // form 表单数据\n\n    /** 表达校验信息 */\n\n    const phoneComplaintRules = ref({\n      type: {\n        required: true,\n        type: 'number',\n        message: \"请选择头投诉类型\",\n        trigger: \"blur\"\n      },\n      complainantsName: {\n        required: true,\n        message: \"请输入投诉人姓名\",\n        trigger: \"blur\"\n      },\n      complainantsPhone: {\n        required: true,\n        message: \"请输入投诉人电话\",\n        trigger: \"blur\"\n      },\n      conEnt: {\n        required: true,\n        message: \"请输入投诉内容\",\n        trigger: \"blur\"\n      }\n    });\n    /** 表单数据 */\n\n    const phoneComplaintFormValue = ref({\n      type: null,\n      complainantsPhone: null,\n      complainantsName: null,\n      conEnt: null\n    });\n    /**\r\n     * 提交投诉信息\r\n     */\n\n    const phoneComplaintSubimt = e => {\n      e.preventDefault();\n      phoneComplaintFormRef.value?.validate(errors => {\n        if (!errors) {\n          // 校验通过\n          if (phoneComplaintModalType.value === 'add') {\n            // 添加\n            addPhoneComplaint();\n          }\n        } else {// 校验不通过\n        }\n      });\n    };\n    /**\r\n     * 添加投诉信息\r\n     */\n\n\n    const addPhoneComplaint = () => {\n      if (housingInfo.value.data == null) {\n        message.warning('请选择房屋信息！');\n      } else {\n        console.log(housingInfo.value.data);\n        const data = {\n          buildingNo: housingInfo.value.data.building.buildingNo,\n          complaintsNo: createId(),\n          createTime: formatDate(new Date()),\n          housesNo: housingInfo.value.data.housesNo,\n          unit: housingInfo.value.data.unit.unitNo\n        };\n        mergeObject(phoneComplaintFormValue.value, data);\n        console.log(phoneComplaintFormValue.value);\n      }\n      /*axios({\r\n          method: 'post',\r\n          url: global.api + '/addPhoneComplaints',\r\n          data: QueryString()\r\n      }).then(res => {\r\n        }).catch(err => {\r\n        }).finally(() => {\r\n        })*/\n\n    }; // 打开模态框\n\n\n    const openModal = (status, item) => {\n      console.log(item); // 清空模态框数据\n\n      phoneComplaintFormValue.value = {\n        type: null,\n        complainantsPhone: null,\n        complainantsName: null,\n        conEnt: null\n      };\n      phoneComplaintOptionShow.value = true;\n      phoneComplaintModalType.value = status;\n    }; // #endregion\n\n\n    const __returned__ = {\n      global,\n      message,\n      findComplaints,\n      typeOption,\n      statusOption,\n      findData,\n      resetData,\n      phoneComplaintData,\n      pageNo,\n      pages,\n      phoneComplaintLoading,\n      findPhoneComplaint,\n      pageChange,\n      phoneComplaintOptionShow,\n      phoneComplaintFormRef,\n      phoneComplaintModalType,\n      housingInfo,\n      phoneComplaintRules,\n      phoneComplaintFormValue,\n      phoneComplaintSubimt,\n      addPhoneComplaint,\n      openModal,\n      ref,\n      computed,\n      inject,\n      NCard,\n      NSpace,\n      NInput,\n      NButton,\n      NTable,\n      NIcon,\n      NPagination,\n      NButtonGroup,\n      NEllipsis,\n      NSelect,\n      NDatePicker,\n      useMessage,\n      NSpin,\n      NForm,\n      NFormItem,\n      NGridItem,\n      NGrid,\n      AddIcon,\n      EditIcon,\n      DeleteIcon,\n      SearchIcon,\n      ArrowClockwiseIcon,\n      CaretDownIcon,\n      HousingOption,\n      axios,\n      mergeObject,\n      createId,\n      formatDate,\n      QueryString\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n});","map":{"version":3,"mappings":"AA0XA,oBAAe;EACbA,IAAI,EAAE;AADO,CAAf;AAhOA,SAAQC,GAAR,EAAaC,QAAb,EAAuBC,MAAvB,QAAoC,KAApC,C,CACA;;AACA,SACEC,KADF,EACSC,MADT,EACiBC,MADjB,EACyBC,OADzB,EACkCC,MADlC,EAEEC,KAFF,EAESC,WAFT,EAEsBC,YAFtB,EAGEC,SAHF,EAGaC,OAHb,EAGsBC,WAHtB,EAGmCC,UAHnC,EAIEC,KAJF,EAISC,KAJT,EAIgBC,SAJhB,EAI2BC,SAJ3B,EAIsCC,KAJtC,QAKO,UALP,C,CAMA;;AACA,SACEC,YAAY,IAAIC,OADlB,EAEEC,aAAa,IAAIC,QAFnB,EAGEC,eAAe,IAAIC,UAHrB,EAIEC,eAAe,IAAIC,UAJrB,EAKEC,uBAAuB,IAAIC,kBAL7B,EAMEC,iBAAiB,IAAIC,aANvB,QAOO,gBAPP,C,CASA;;AACA,OAAOC,aAAP,MAA0B,oCAA1B;AAEA,OAAOC,KAAP,MAAkB,OAAlB;AACA,SAAQC,WAAR,EAAqBC,QAArB,EAA+BC,UAA/B,QAAgD,qBAAhD;AACA,OAAOC,WAAP,MAAwB,IAAxB,C,CAEA;;;;;;cA1BA;;IA2BA,MAAMC,MAAM,GAAGpC,MAAM,CAAC,QAAD,CAArB,EACA;;IACA,MAAMqC,OAAO,GAAGzB,UAAU,EAA1B,EAEA;;IACA,MAAM0B,cAAc,GAAGxC,GAAG,CAAC;MACzByC,YAAY,EAAE,IADW;MAEzBC,MAAM,EAAE,IAFiB;MAGzBC,IAAI,EAAE;IAHmB,CAAD,CAA1B,EAMA;;IACA,MAAMC,UAAU,GAAG,CAAC,IAAD,EAAO,IAAP,EAAa,IAAb,EAAmBC,GAAnB,CAAuB,CAACC,CAAD,EAAIC,CAAJ,MAAW;MACnDC,KAAK,EAAEF,CAD4C;MAEnDG,KAAK,EAAEF;IAF4C,CAAX,CAAvB,CAAnB,EAKA;;IACA,MAAMG,YAAY,GAAG,CAAC,KAAD,EAAQ,KAAR,EAAeL,GAAf,CAAmB,C